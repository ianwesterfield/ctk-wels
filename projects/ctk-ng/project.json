{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "ctk-ng",
  "prefix": "ctkng",
  "projectType": "library",
  "sourceRoot": "projects/ctk-ng/src",
  "tags": [],
  "targets": {
    "build": {
      "configurations": {
        "development": {
          "tsConfig": "projects/ctk-ng/tsconfig.lib.json"
        },
        "production": {
          "tsConfig": "projects/ctk-ng/tsconfig.lib.prod.json"
        }
      },
      "defaultConfiguration": "production",
      "executor": "@nx/angular:package",
      "options": {
        "project": "projects/ctk-ng/ng-package.json"
      },
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "projects/ctk-ng/jest.config.ts"
      },
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ]
    }
  }
}
